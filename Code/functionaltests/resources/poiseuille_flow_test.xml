<?xml version="1.0" ?>
<hemelbsettings>
  <simulation cycles="8" cyclesteps="1000" stresstype="1" />
  <geometry>
    <datafile path="poiseuille_flow_test.gmy" />
  </geometry>
  <inlets>
    <inlet>
      <pressure amplitude="0.0" mean="16.0" phase="0.0" />
      <normal x="0.0" y="0.0" z="1.0" />
      <position x="0.0" y="0.0" z="-0.03" />
    </inlet>
  </inlets>
  <outlets>
    <outlet>
      <pressure amplitude="0.0" mean="0.0" phase="0.0" />
      <normal x="0.0" y="0.0" z="-1.0" />
      <position x="0.0" y="0.0" z="0.03" />
    </outlet>
  </outlets>
  <visualisation>
    <centre x="0.0" y="0.0" z="0.0" />
    <orientation latitude="45.0" longitude="45.0" />
    <display brightness="0.03" zoom="1.0" />
    <range maxstress="0.1" maxvelocity="0.1" />
  </visualisation>
  <properties>
    <propertyoutput frequency="1000" file="40mmlinevelocity.dat">
      <linegeometry>
        <point x="-0.75e-3" y="0" z="10e-3"/>
        <point x="0.75e-3" y="0" z="10e-3"/> 
        <field type="velocity" name="Velocity field 40mm into the tube"/>
      </linegeometry>    
    </propertyoutput>
    <propertyoutput frequency="8000" file="wholegeometryvelocity.dat">
      <wholegeometry>
        <field type="velocity" name="Whole velocity field"/>
      </wholegeometry>      
    </propertyoutput>        

    <propertyoutput frequency="1000" file="40mmlineshearstress.dat">
      <linegeometry>
        <point x="-0.75e-3" y="0" z="10e-3"/>
        <point x="0.75e-3" y="0" z="10e-3"/> 
        <field type="shearstress" name="Shear stress 40mm into the tube"/>
      </linegeometry>    
    </propertyoutput>
  </properties>
</hemelbsettings>
